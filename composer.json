{
    "name": "sofyco/google-authenticator-bundle",
    "type": "symfony-bundle",
    "description": "Symfony Google Authenticator Bundle",
    "keywords": ["php", "symfony", "bundle", "security", "google", "authenticator"],
    "homepage": "https://github.com/sofyco/google-authenticator-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Sofy Korzhova",
            "email": "sofykorzhova@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "google/apiclient": "^2.12",
        "sofyco/jwt-authenticator-bundle": "^1.0",
        "symfony/framework-bundle": "^6.2",
        "symfony/messenger": "^6.2",
        "symfony/security-bundle": "^6.2"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.9",
        "phpunit/phpunit": "^9.5",
        "symfony/browser-kit": "^6.2",
        "symfony/yaml": "^6.2"
    },
    "autoload": {
        "psr-4": {
            "Sofyco\\Bundle\\GoogleAuthenticatorBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sofyco\\Bundle\\GoogleAuthenticatorBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
